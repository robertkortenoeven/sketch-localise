{
  "name": "App Store Export",
  "description" : "Localises Sketch files using Mac and iOS localisable.strings files",
  "author": "Robert Kortenoeven",
  "authorEmail" : "robert@welivelikethis.net",
    "homepage": "http://welivelikethis.net",
  "identifier" : "PLUGIN_IDENTIFIER",
  "compatibleVersion": 44,
  "build": "PLUGIN_BUILD",
  "version": "PLUGIN_VERSION",
  
  "commands": [
   {
     "name" : "Run",
     "identifier" : "run",
     "shortcut" : "",
     "script" : "script.cocoscript",
     "handler" : "onRun"
   },
   {
     "script" : "script.cocoscript",
     "handlers" : {
       "actions" : {
         "Startup" : "onStartup",
         "OpenDocument":"onOpenDocument",
         "SelectionChanged.finish" : "onSelectionChanged"
       }
     }
   },
   {
     "name" : "Select iOS Folder",
     "identifier" : "AppStoreExport.plugin.selectIOS",
     "shortcut" : "command shift i",
     "script" : "script.cocoscript",
     "handler" : "selectIOSFolder"
   },
   {
     "name" : "Select Android Folder",
     "identifier" : "AppStoreExport.plugin.selectAndroid",
     "shortcut" : "",
     "script" : "script.cocoscript",
     "handler" : "selectAndroidFolder"
   },
   {
     "script" : "StoreExportPanel.framework/StoreExportPanel.js",
     "handlers" : {
       "actions" : {
         "Startup" : "onStartup",
         "OpenDocument":"onOpenDocument",
         "SelectionChanged.finish" : "onSelectionChanged"
       }
     }
   }
 ],
  "menu": {
    "isRoot" : false,
    "shortcut" : "",
    "title": "App Store Export",
    "items": [
      "run"
    ]
  }
}
